function main(args) {
  match a {
    case "hello": a;
    case [1, 2, 3] when a: b;
    case [1, ...b]: b;
    case { a: 1, b: c }: c;
    case Nothing(): e;
    case Just(a): a;
    default: b;
  }
}

class Base() {}
data class Greet(who, what) extends Base() {
  static _.hello() { who; }
  member _.whose { who; }
  member _.whose <- that { who; }
}